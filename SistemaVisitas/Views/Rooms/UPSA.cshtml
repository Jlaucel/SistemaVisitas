                        @Html.ValidationMessageFor(m => m.VOLTAJE_SALIDA_L12, "", new { @class = "text-danger" })
                    </div>
                    <div class=" col-md-1">
                        <label for="VOLTAJE_SALIDA_L23" class=" form-control-label">L23</label>
                    </div>
                    <div class=" col-md-3">

                        @Html.TextBoxFor(m => m.VOLTAJE_SALIDA_L23, new { @class = "form-control", @placeholder = "°" })
                        @Html.ValidationMessageFor(m => m.VOLTAJE_SALIDA_L23, "", new { @class = "text-danger" })
                    </div>
                    <div class=" col-md-1">
                        <label for="VOLTAJE_SALIDA_L31" class=" form-control-label">L31</label>
                    </div>
                    <div class=" col-md-3">

                        @Html.TextBoxFor(m => m.VOLTAJE_SALIDA_L31, new { @class = "form-control", @placeholder = "°" })
                        @Html.ValidationMessageFor(m => m.VOLTAJE_SALIDA_L31, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="row form-group" style="padding-left:2em;">
                    
                        <label class=" form-control-label"><strong>Alertas en los Hardware </strong></label>
                    
                </div>

                         
                <div class="row form-group" style="padding-left:2em;">
                   <div class="col-xs-2 col-sm-2 col-md-2">
                        <div class="checkbox">
                            <small>Rack 1</small>
                        </div>
                    </div>
                    <label class="switch switch-text switch-primary">
                        @Html.CheckBoxFor(m => m.RACK1_STATUS, new { @class = "switch-input", @checked = "checked" })
                        <span data-on="OK" data-off="NO OK" class="switch-label" style="width:60px;"> </span>
                        <span class="switch-handle" style="width:25px;"></span>
                    </label>
                    <div class="col-xs-3 col-sm-3 col-md-3">

                        <button id="detailBtn" value="1" type="button" class="btn-sm btn btn-secondary mb-1" data-toggle="modal" data-target="#staticModal">
                            Detalle Rack
                        </button>
                    </div>
                    
                </div>
          
            <div class="card-footer">
                <button type="submit" class="btn btn-primary btn-sm">
                    <i class="fa fa-dot-circle-o"></i>  

                </button>
                <button type="reset" class="btn btn-danger btn-sm">
                    <i class="fa fa-ban"></i> Reset
                </button>
            </div>
        </div>
    </div>
